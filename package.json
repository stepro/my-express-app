{
  "name": "my-express-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2"
  },
  "scripts": {
    "watch": "nodemon ./bin/www",
    "debug": "node --debug=5858 --debug-brk --nolazy ./bin/www",
    "debug-remote": "node --debug=0.0.0.0:5858 --debug-brk --nolazy ./bin/www",
    "start": "node ./bin/www",
    "azp": "azp up --sync /app npm run",
    "azp-debug": "azp up -p 5858:5858 --sync /app npm run debug-remote"
  }
}
